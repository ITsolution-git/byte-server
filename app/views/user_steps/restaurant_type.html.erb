<div class="well">
  <div class="row-fluid">
    <div class="span12">
      <center><%= image_tag 'signup/brand.png' %></center>
    </div>
    <div class="span12">
      <h3 class="text-center">You Are Primary A..</h3>
    </div>
  </div>

  <!-- star form -->
  <%= form_for(resource, :url => wizard_path, method: :put, :html => {:autocomplete => "off", :class => "restaurant_type_form"}) do |f| %>

  <div class="row-fluid">
    <div class="span3">
      <div class="children restaurant_type">
        <center><%= link_to (image_tag 'signup/left_btn.png'), new_user_registration_path(:fromstep2 => true) %></center>
      </div>
    </div>
    <div class="span6">
      <%
        if !session["move_location_id"].blank?
          @location = Location.find(session["move_location_id"])
        end
      %>
      <%= render 'shared/error_messages', :object => resource %>
      <div id="restaurant-types" style="display: <%= session["move_location_id"].blank? ? 'block' : 'none' %>;">
        <div class="row-fluid top10">
          <div class="span6">
          	<%= image_tag 'signup/2_retaraunt.png', data: { type: 'restaurant' }  %>
          </div>
          <div class="span6">
          	<%= image_tag 'signup/2_mobile_truck.png', data: { type: 'mobile_truck' }  %>
          </div>
        </div>
        <div class="row-fluid top10">
          <div class="span6">
            <%= image_tag 'signup/2_catering.png', data: { type: 'catering' }  %>
          </div>
          <div class="span6">
            <%= image_tag 'signup/2_vendor.png', data: { type: 'vendor' } %>
          </div>
        </div>
       <%= f.hidden_field :restaurant_type %>
      </div>

      <div class="row-fluid top10" style="margin-top:20px;">
        <div class="span12">
         <!--  <%= f.submit "Continue", :class=> "btn btn-primary", :id=>'submit_form_register'%> -->
        </div>
      </div>
    </div>
    <div class="span3">
      <div class="children restaurant_type">
        <center><%= image_submit_tag 'signup/right_btn.png' %></center>
      </div>
    </div>
  </div>
  <% end %>
  <!-- end of form --> 
  <div class="row-fluid">
    <div class="span12">
      <center><%= image_tag 'signup/status_2.png' %></center>
    </div>
  </div>
</div>
