<h1>Listing admin_pages</h1>

<table class="table .table-striped">
  <tr>
    <th>Title</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @pages.each do |page| %>
  <tr>
    <td><%= page.title %></td>
    <td><%= link_to 'Edit', edit_admin_page_path(page) %></td>
    <td><%#= link_to 'Show', admin_page_path(page) %></td>
    <td><%#= link_to 'Destroy', admin_page_path(page), method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

