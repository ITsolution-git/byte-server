<div class="block-head clearfix header-menu-mgt">
  <span class="<%= load_toogle_icon(item_key) %>" data-toggle="collapse" data-target="#itemkey_toggle"></span>
  <span class="wrap_title_form">
    <%= render partial: 'menus/item_key_title_form', locals: {item_key: item_key} %>

  </span>

  <% if @tutorialVideos.present? %>
    <% if ( v = @tutorialVideos.find { |v| v['title'] == "Create Menu Item Keys" } ) && ( v['url'] != '' ) %>
      <% @hasVideo = true %>
      <% @url = v['url'] %>
    <% else %>
      <% @hasVideo = false %>
      <% @url = '' %>
    <% end %>
  <% end %>

  <% if @hasVideo %>
    <%= image_tag("play_icon.png", :class => "playTutorial playTutorialBtn floatright", :id => '4') %>
  <% end %>

  <%= link_to "", "#", :class => "icon-question-sign btn floatright", :id => 'item_key_tooltip', :title => load_content_of_file('/public/tooltip/menukeys.html'), :"data-toggle"=>"tooltip", "data-placement "=> "right", "data-html" => "true", :rel => "tooltip", :style => "color:#333;" %>

  <div class="tutorialVideo" id="4" style="display:none;">
    <br>
    <iframe width="100%" height="400" src="<%= @url %>" frameborder="0" allowfullscreen></iframe>
  </div>

</div>
<div id="itemkey_toggle"  class="collapse <%= toogle_state(item_key) %>">
  <%= render partial: 'menus/item_key_form_primary', locals: {item_key: item_key, item_key_image: item_key_image} %>
</div>
<script type="text/javascript">
  $('#item_key_tooltip').tooltip();
</script>
