<div class="block-head clearfix header-menu-mgt">
  <span class="<%= load_toogle_icon(item) %>  btn togglebtn" data-toggle="collapse" data-target="#item_form_toggle"></span>
   <span class="wrap_title_form">
    <%= render partial: 'menus/item_title_form', locals: {item: item} %>
   </span>

  <% if ( !@tutorialVideos.nil? && v = @tutorialVideos.find { |v| v['title'] == "Create Menu Items" } ) && ( v['url'] != '' ) %>
    <% @hasVideo = true %>
    <% @url = v['url'] %>
  <% else %>
    <% @hasVideo = false %>
    <% @url = '' %>
  <% end %>

  <% if @hasVideo %>
    <%= image_tag("play_icon.png", :class => "playTutorial playTutorialBtn floatright", :id => "6") %>
  <% end %>

  <%= link_to "","#",:class=>"icon-question-sign btn floatright", :id => 'item_form_tooltip', :title => load_content_of_file('/public/tooltip/menuitem.html'),:"data-toggle"=>"tooltip","data-placement"=>"right","data-html"=>"true",:rel=>"tooltip",:style=>"color:#333;" %>

  <div class="tutorialVideo" id="6" style="display:none;">
    <br>
    <iframe width="100%" height="400" src="<%= @url %>" frameborder="0" allowfullscreen></iframe>
  </div>

</div>
<div id="item_form_toggle"  class="collapse <%= toogle_state(item) %>">
  <%= render partial: 'menus/item_form_primary', locals: {item: item, item_image: item_image} %>
</div>
<script type="text/javascript">
  $("#item_form_tooltip").tooltip();
</script>
